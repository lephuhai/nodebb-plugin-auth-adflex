{
  "id": "nodebb-plugin-auth-adflex",
  "library": "./library.js",
  "hooks": [
    {
      "hook": "action:auth.overrideLogin",
      "method": "login"
    },
    {
      "hook": "static:app.load",
      "method": "load"
    },
    {
      "hook": "action:user.loggedIn",
      "method": "redirectToConfirm"
    }
  ],
  "templates": "./templates"
}
